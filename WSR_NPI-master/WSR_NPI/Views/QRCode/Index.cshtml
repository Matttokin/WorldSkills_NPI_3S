@model WSR_NPI.Models.Picture

@{
    ViewBag.Title = "Распозновать данные";
}

@using (Html.BeginForm("Index", "QRCode", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Добавить изображение</legend>

        <div class="editor-label">Файл изображения</div>
        <div class="editor-field">
            <input type="file" name="uploadImage" />
        </div>

        <p>
            <input type="submit" class="order-btn" value="Распознать" />
        </p>
    </fieldset>
}

<div class="details">
    @if (Model != null)
    {
        @(Model.Name != null ? Model.Name : "")
    }
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

